{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "server=localhost;port=3306;user=root;password=;database=unpak_cbt;" //host.docker.internal
  },
  "Redis": {
    "ConnectionString": "host.docker.internal:6379"
  },
  "Minio": {
    "Endpoint": "storage:9001"
  },
  "Jwt": {
    "Sub": "localhost",
    "Issuer": "localhost",
    "Audience": "localhost",
    "Secret": "UnpakCBTSecureKey1234567890!@$%#*&^"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://host.docker.internal:5341" } //https://seb.unpak.ac.id/monitoring
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "UnpakCbt.Api"
    }
  },
  "AllowedHosts": "*"
}
